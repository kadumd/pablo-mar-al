{
    "version": 2,
    "builds": [
        {
            "src": "/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/index.css",
            "dest": "/index.css"
        },
        {
            "src": "/index.js",
            "dest": "/index.js"
        }
    ]
}